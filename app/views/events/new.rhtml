<h1>New event</h1>

<%= error_messages_for :event %>

<% form_for(:event, :url => events_path, :html => {:onsubmit => "SelectAll(document.getElementById('remove_users')); SelectAll(document.getElementById('add_users'))"}) do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
	<%= submit_tag "Create" %>
<% end %>

<%= link_to 'Back', events_path %>