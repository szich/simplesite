<h1>New resource</h1>

<%= error_messages_for :resource %>

<% form_for(:resource, :html => { :target => 'frame' }, :url => resources_path) do |f| %>
	<%= render :partial => 'form', :locals => { :f => f } %>
  <%= submit_tag "Create" %> <input type="button" value="Cancel" onclick="lightbox.prototype.deactivate();" />
<% end %>

<a href="#" class="lbAction" rel="deactivate">Back</a>

<iframe id='frame' name="frame" width="0" height="0"></iframe>

