<h1>New Member</h1>

<%= error_messages_for :user %>
<% form_for :user, :url => users_path do |f| -%>

<h3 style="margin-bottom: 2px;">Login Preferences</h3>
<%= render :partial => 'login_preferences', :locals => {:f => f} %>

<%= render :partial => 'form', :locals => { :f => f } %>

<p><%= submit_tag 'Sign up' %></p>
<% end -%>
